---
 packages/ffmpeg.cmake | 3 +++
 1 file changed, 3 insertions(+)

diff --git a/packages/ffmpeg.cmake b/packages/ffmpeg.cmake
index aeada10..2230664 100644
--- a/packages/ffmpeg.cmake
+++ b/packages/ffmpeg.cmake
@@ -44,6 +44,9 @@ ExternalProject_Add(ffmpeg
     GIT_REPOSITORY https://github.com/FFmpeg/FFmpeg.git
     SOURCE_DIR ${SOURCE_LOCATION}
     GIT_CLONE_FLAGS "--filter=tree:0"
+    GIT_REMOTE_NAME origin
+    GIT_TAG master
+    GIT_RESET 4adbe2d219c13d5342d073aace5001a64c57d9c8
     UPDATE_COMMAND ""
     CONFIGURE_COMMAND ${EXEC} <SOURCE_DIR>/configure
         --cross-prefix=${TARGET_ARCH}-
